        "n = \"mom\"\n",
        "j = len(n) - 1\n",
        "for i in range(len(n)):\n",
        "  if n[i] == n[j]:\n",
        "    j-=1\n",
        "    if j < i:\n",
        "      print(\"match\")\n",
        "      break\n",
        "  elif n[i] != n[j]:\n",
        "    print(\"not a match\")\n",
        "    break"
      ]
